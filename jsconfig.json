// jsconfig.json
{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "paths": {
        "assets/*": ["src/assets/*"],
        "ui/*": ["src/components/ui/*"],
        "components/*": ["src/components/*"],
        "reducers/*": ["src/reducers/*"],
        "actions/*": ["src/actions/*"],
        "containers/*": ["src/containers/*"],
        "constants/*": ["src/constants/action-types.js"],
      },
  }
}
